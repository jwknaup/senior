	pages = {021012--021012--12},
	file = {Full Text PDF:C\:\\Users\\Jacob\\Zotero\\storage\\5G6UFIGG\\Ramezani et al. - 2013 - Performance Analysis and Feedback Control of ATRIA.pdf:application/pdf}
}

