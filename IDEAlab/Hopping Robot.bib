	pages = {2128--2134},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Jacob\\Zotero\\storage\\5FVXSKWI\\7759334.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Jacob\\Zotero\\storage\\S5UMJVKZ\\Li et al. - 2016 - Cockroach-inspired winged robot reveals principles.pdf:application/pdf}
