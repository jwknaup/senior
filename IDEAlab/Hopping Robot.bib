	pages = {2971--2976},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Jacob\\Zotero\\storage\\EFX3VXDS\\6907287.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Jacob\\Zotero\\storage\\CNE8S6QD\\Agheli et al. - 2014 - Design and fabrication of a foldable hexapod robot.pdf:application/pdf}
