	pages = {4757--4764},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Jacob\\Zotero\\storage\\7IE7U7FM\\7759699.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Jacob\\Zotero\\storage\\UXNC4N6C\\Haldane et al. - 2016 - A power modulating leg mechanism for monopedal hop.pdf:application/pdf}
