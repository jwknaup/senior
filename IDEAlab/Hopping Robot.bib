	pages = {2546--2551},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Jacob\\Zotero\\storage\\548NVKDJ\\4209466.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Jacob\\Zotero\\storage\\WVTHCNK2\\Niiyama et al. - 2007 - Mowgli A Bipedal Jumping and Landing Robot with a.pdf:application/pdf}
